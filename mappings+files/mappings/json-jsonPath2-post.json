{
  "request": {
    "method": "POST",
    "urlPath": "/jsonPath2",
    "bodyPatterns": [
      {
        "equalToJson": "{\"username\":\"admin\",\"password\":\"test\",\"rememberMe\":false}",
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200,
    "body": "{ \"keywords\": {{{jsonPath request.body '$'}}} }",
    "transformers": ["response-template"],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
